
.title {
	width: 100%;
	height: 40px;
	background-color: rgb(249, 249, 249);
	font-size: 18px;
	line-height: 40px;
	font-weight: 600;
	padding: 0 20px;
}
.content {
	width: 100%;
}
._ul {
	width: 100%;
	padding: 0;
	/* overflow: hidden; */
}
.content ._li {
	float: left;
	list-style: none;
	width: 25%;
	height: 105px;
	text-align: center;
	margin: 0 auto;
}
._li image {
	margin-top: 10px;
	width: 37px;
	height: 37px;
}
._li ._p {
	display: block;
	/* width: 90px; */
	width: 90%;
	/* margin: 0 auto; */
	text-align: center;
	font-size: 13px
}
.one {
	margin-top: 35px;
}
.clearfix:after {
	/*伪元素是行内元素 正常浏览器清除浮动方法*/
	content: "";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
}

